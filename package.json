  "scripts": {
    "clean:electron": "rm -rf dist out",
    "clean:android": "rm -rf android/.gradle android/app/build android/app/src/main/assets",
    "clean": "yarn clean:electron && yarn clean:android",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:start": "electron-vite preview",
    "electron:dev": "electron-vite dev",
    "electron:build": "electron-vite build",
    "electron:postinstall": "electron-builder install-app-deps",
    "electron:build:win": "yarn clean && yarn electron:build && electron-builder --win --config",
    "electron:build:mac": "yarn clean && yarn electron:build && electron-builder --mac --config",
    "electron:build:linux": "yarn clean && yarn electron:build && electron-builder --linux --config",
    "capacitor:copy:assets": "capacitor-assets generate --android --iconBackgroundColor '#eeeeee' --iconBackgroundColorDark '#222222' --splashBackgroundColor '#eeeeee' --splashBackgroundColorDark '#111111'",
    "capacitor:prepare:android": "yarn build && yarn capacitor:copy:assets && cap sync",
    "capacitor:run:android": "yarn capacitor:prepare:android && cap run android",
    "capacitor:build:android": "yarn capacitor:prepare:android && env-cmd cap build android",
    "build:desktop": "yarn clean && yarn electron:build && electron-builder -mwl --config",
    "build:mobile": "yarn clean && yarn capacitor:build:android"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
